{
  "expo": {
    "scheme": "instakash",
    "facebookScheme": "fb285266469843350",
    "facebookAppId": "285266469843350",
    "facebookDisplayName": "My kash business",
    "facebookAutoLogAppEventsEnabled": true,
    "facebookAdvertiserIDCollectionEnabled": true,
    "name": "Instakash",
    "slug": "instakash-app",
    "version": "0.2.2",
    "platforms": ["ios", "android"],
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D8284"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "instakash-sac",
            "project": "instakash-sac",
            "authToken": "f9aea168dc174991a8ada3492ece7e658b22ce3f8a094c1b9420d9b9b80fa299"
          }
        }
      ]
    },
    "plugins": [
      "expo-camera",
      "sentry-expo",
      "expo-notifications",
      "expo-community-flipper",
      "expo-facebook",
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ]
    ],
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "associatedDomains": ["applinks:app.instakash.net"],
      "bundleIdentifier": "instakash.app",
      "buildNumber": "0.1.1",
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera to give you the ability to take the photo of your document."
      }
    },
    "android": {
      "package": "net.instakash.app",
      "versionCode": 29,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-fr.png",
        "backgroundImage": "./assets/adaptive-bg.png"
      },
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "app.instakash.net",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "permissions": ["CAMERA", "ACCESS_COARSE_LOCATION", "WRITE_SETTINGS", "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE", "RECEIVE_BOOT_COMPLETED"]
    }
  }
}

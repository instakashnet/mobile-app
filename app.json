{
  "expo": {
    "scheme": "instakash",
    "name": "Instakash",
    "slug": "instakash-app",
    "version": "0.0.6",
    "orientation": "portrait",
    "platforms": ["ios", "android"],
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D8284"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "instakash-sac",
            "project": "instakash-sac",
            "authToken": "f9aea168dc174991a8ada3492ece7e658b22ce3f8a094c1b9420d9b9b80fa299"
          }
        }
      ]
    },
    "plugins": ["expo-camera", "sentry-expo", "expo-notifications", "expo-community-flipper"],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "associatedDomains": ["applinks:app.instakash.net"],
      "bundleIdentifier": "instakash.app",
      "buildNumber": "0.1.2",
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera to give you the ability to take the photo of your document."
      }
    },
    "android": {
      "package": "net.instakash.app",
      "versionCode": 18,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-fr.png",
        "backgroundImage": "./assets/adaptive-bg.png"
      },
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "app.instakash.net",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "permissions": ["CAMERA", "ACCESS_COARSE_LOCATION", "WRITE_SETTINGS", "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE", "RECEIVE_BOOT_COMPLETED"]
    }
  }
}
